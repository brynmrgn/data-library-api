<head>
		<meta charset="UTF-8">
	    <meta name="format-detection" content="telephone=no">
	    <meta name="viewport" content="width=device-width">
		<title>Data Library Explorer</title>
		<link rel="canonical" href="<%= url_for(:only_path => false, :protocol => 'https') %>">
		<% if @csv_url -%>
			<link rel="alternate" href="<%= @csv_url %>" type="text/csv">
		<% end -%>
		<% if @rss_url -%>
			<link rel="alternate" href="<%= @rss_url %>" type="application/rss+xml">
		<% end -%>
		<meta property="og:locale" content="en_GB" >
		<meta property="og:type" content="website" >
		<meta property="og:url" content="" >
		<meta property="og:site_name" content="UK Parliament Data Library explorer" >
		<%= favicon_link_tag asset_path('favicon.ico') %>
 		<%= stylesheet_link_tag "application" %>
		<style>
		#sparql textarea {
			height: 500px;
			width: 100%;
			}	
		header {
    		background-color: #ffffff;
    		color: #373151;
			}	
		header .product-header .title {
			color: #373151;
		}
		path {
			fill-rule: evenodd;
			clip-rule: evenodd;
			fill: #373151;
				}
		.pagy-nav {
  display: flex;
  align-items: center;
  gap: .5rem;
}
.pagination-wrapper {
  display: flex !important;
  align-items: center !important;
  justify-content: space-between !important;
  margin: 1rem 0 !important;
  padding: 0.75rem !important;
  background-color: #f5f5f5 !important;
  border-radius: 4px !important;
}

.pagination-info p {
  margin: 0 !important;
  color: #666 !important;
  font-size: 1.2rem !important;
}

.pagination-nav {
  margin-left: 2rem !important;
}

/* Style the pagination links */
.pagination-nav nav a,
.pagination-nav nav span {
  font-size: 1.2rem !important;
  margin: 0 0.01rem !important;
  padding: 0.25rem 0.5rem !important;
}

		</style>
		<%= csrf_meta_tags %>
		<%= csp_meta_tag %>
	</head>