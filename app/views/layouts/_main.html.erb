	<main id="main" class="main-content">
		<div>
			<div class="container">
        <% if content_for?(:title) %>
	        <h2>
	          <%= yield :title %>
	        </h2>
        <% end %>
        <%= content_for?(:content) ? yield(:content) : yield %>
      </div>
      <%= render :partial => 'layouts/queries/queries', :object => @queries unless @queries.empty? %>
    </div>
  </main>